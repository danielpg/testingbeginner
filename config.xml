<?xml version="1.0" encoding="UTF-8" ?>
<widget xmlns   = "http://www.w3.org/ns/widgets"
		xmlns:gap   = "http://phonegap.com/ns/1.0"
		id          = "com.phonegap.colores"
		versionCode = "1" 
		version     = "1.0.0" >

	<!-- versionCode is optional and Android only -->

	<name>Mood Test</name>

	<description>
	   Mood tester for tablets and phones.
	</description>

	<author href="https://build.phonegap.com" email="support@phonegap.com">
		Hardeep Shoker 
	</author>

	<feature name="http://api.phonegap.com/1.0/file"/>
	<feature name="http://api.phonegap.com/1.0/geolocation"/>
	<!--feature name="http://api.phonegap.com/1.0/camera"/-->
	<feature name="http://api.phonegap.com/1.0/network"/>

	<gap:plugin name="com.brodysoft.sqliteplugin" version="1.0.0" />
	<!--gap:plugin name="org.apache.cordova.camera" version="0.3.2" /-->

	<gap:splash src="splash.png" />
	<icon src="icon.png" />

	<feature name="Camera">
		<param name="android-package" value="org.apache.cordova.camera.CameraLauncher" />
	</feature>

	<access origin="http://adostelecom.com" />

</widget>
